{
  "manifest_version": 3,
  "name": "MoodLink",
  "version": "1.0",
  "description": "It identifies the mood of the meeting attendies when turned on and at the end gives a meeting summary in the end.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Take Screenshot"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*",
    "<all_urls>"
  ]
}
